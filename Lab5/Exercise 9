package com.company;

public class Exercise9 {
    public static void main(String[] args)
    {
        String columnist1 ="Задание девять";
        String columnist2 ="Управжнение номер девять";
        String columnist3 ="Номер";
        boolean equals1 = columnist1.equals(columnist2);
        boolean equals2 = columnist1.equals(columnist3);
        // Display the results of the equality checks.
        System.out.println("\"" + columnist1 + "\" равно \"" +
                columnist2 + "\"? " + equals1);
        System.out.println("\"" + columnist1 + "\" равно \"" +
                columnist3 + "\"? " + equals2);
    }
}
